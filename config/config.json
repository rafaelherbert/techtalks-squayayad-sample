{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "admin-application-customizer": {
      "components": [
        {
          "entrypoint": "./lib/extensions/admin/AdminApplicationCustomizer.js",
          "manifest": "./src/extensions/admin/AdminApplicationCustomizer.manifest.json"
        }
      ]
    },
    "banner-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/banner/BannerWebPart.js",
          "manifest": "./src/webparts/banner/BannerWebPart.manifest.json"
        }
      ]
    },
    "faq-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/faq/FaqWebPart.js",
          "manifest": "./src/webparts/faq/FaqWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "AdminApplicationCustomizerStrings": "lib/extensions/admin/loc/{locale}.js",
    "ControlStrings": "node_modules/@pnp/spfx-controls-react/lib/loc/pt-pt.js",
    "PropertyControlStrings": "node_modules/@pnp/spfx-property-controls/lib/loc/{locale}.js",
    "BannerWebPartStrings": "lib/webparts/banner/loc/{locale}.js",
    "FaqWebPartStrings": "lib/webparts/faq/loc/{locale}.js"
  }
}
